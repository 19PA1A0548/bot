	}

	function closeNav() {
  		document.getElementById("mySidenav").style.width = "0";
  		document.getElementById("main").style.marginLeft= "0";
	}
	var story = "We put design at core of our themes and.tailor them to those who makes thing happen";
	var bod = document.getElementById("downtext");
	var br = "<br>";
	setInterval(add,50);
  	var i =0;
  	function add(){
  		if(story[i]=="."){
    		bod.innerHTML += br;
    		i++;
  		}
  		if(i<story.length){
    		bod.innerHTML+=story[i];
    		i++
    	}
  	}

  	$(".slider").owlCarousel({
        loop: true,
        autoplay: true,
        autoplayTimeout: 2000, //2000ms = 2s;
        autoplayHoverPause: true,
