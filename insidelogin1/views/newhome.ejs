		}
		.card .content .btn{
		  display: block;
		  text-align: left;
		  margin: 10px 0;
		}
		.card .content .btn button{
		  background: #2196f3;
		  color: white;
		  border: none;
		  outline: none;
		  font-size: 15px;
		  padding: 5px 8px;
		  border-radius: 15px;
		  cursor: pointer;
		  transition: 0.2s;
		}
		.card .content .btn button a{
			text-decoration: none;
			color:white;
		}
		.card .content .btn button:hover{
